-- POSDBA.OWNERS
CREATE TABLE OWNERS(
  OW_CODE   NCHAR(3),
  OW_NAME   NVARCHAR2(5)
)TABLESPACE USERS;
  -- CONSTRAINTS
  ALTER TABLE OWNERS
  ADD CONSTRAINT OW_CODE_PK PRIMARY KEY(OW_CODE)
  MODIFY OW_NAME    NOT NULL;
  -- SYNONYM
  CREATE PUBLIC SYNONYM OW FOR POSDBA.OWNERS;
  -- GRANT
  GRANT SELECT, INSERT, UPDATE ON POSDBA.OWNERS TO POSDEV;
  
  -- DATA << DEV
  INSERT INTO OW(OW_CODE, OW_NAME) VALUES('W01', '김오너');
  INSERT INTO OW(OW_CODE, OW_NAME) VALUES('W02', '이오너');
  INSERT INTO OW(OW_CODE, OW_NAME) VALUES('W03', '정오너');
  INSERT INTO OW(OW_CODE, OW_NAME) VALUES('W04', '박오너');
  INSERT INTO OW(OW_CODE, OW_NAME) VALUES('W05', '최오너');
  COMMIT;
  
  SELECT * FROM OW;
  
-- POS.STORES
CREATE TABLE STORES(
  SR_OWCODE     NCHAR(3),
  SR_CODE       NCHAR(10),
  SR_NAME       NVARCHAR2(50),
  SR_LOCATION   NVARCHAR2(100),
  SR_PHONE      NVARCHAR2(11)
)TABLESPACE USERS;
  -- CONSTAINTS
  ALTER TABLE STORES
  ADD CONSTRAINT SR_OWCODE_CODE_PK  PRIMARY KEY(SR_OWCODE, SR_CODE)
  ADD CONSTRAINT SR_OWCODE_FK   FOREIGN KEY(SR_OWCODE) REFERENCES OWNERS(OW_CODE)
  MODIFY SR_NAME        NOT NULL
  MODIFY SR_LOCATION    NOT NULL
  MODIFY SR_PHONE       NOT NULL;
  -- SYNONYM
  CREATE PUBLIC SYNONYM SR FOR POSDBA.STORES;
  -- GRANT 
  GRANT SELECT, INSERT, UPDATE ON POSDBA.STORES TO POSDEV;
  
  -- DATA << POSDEV
  INSERT INTO SR(SR_OWCODE, SR_CODE, SR_NAME, SR_LOCATION, SR_PHONE) VALUES( 'W01', '1234567890', '젤싸편의점', '인천광역시 미추홀구 학익1동', '03212341234');
  INSERT INTO SR(SR_OWCODE, SR_CODE, SR_NAME, SR_LOCATION, SR_PHONE) VALUES( 'W02', '2345678901', '쫌싸편의점', '인천광역시 미추홀구 숭의동', '03223452345');
  INSERT INTO SR(SR_OWCODE, SR_CODE, SR_NAME, SR_LOCATION, SR_PHONE) VALUES( 'W03', '3456789012', '비싸편의점', '인천광역시 미추홀구 학익2동', '03234123412');
  INSERT INTO SR(SR_OWCODE, SR_CODE, SR_NAME, SR_LOCATION, SR_PHONE) VALUES( 'W04', '4567890123', '쫌비싸편의점', '인천광역시 미추홀구 문학동', '03241234123');
  INSERT INTO SR(SR_OWCODE, SR_CODE, SR_NAME, SR_LOCATION, SR_PHONE) VALUES( 'W05', '5678901234', '할인편의점', '인천광역시 미추홀구 구월동', '03251235123');
