  SELECT * FROM SR;
  
  -- COLUMN APPEND
  ALTER TABLE STORES
  ADD SR_SECRETCODE NVARCHAR2(20);
  
  UPDATE SR SET SR_SECRETCODE = '4321';
  COMMIT;
  
  ALTER TABLE STORES
  MODIFY SR_SECRETCODE NOT NULL;
  
  SELECT * FROM SR;
   
-- POSDBA.MEMBERS
CREATE TABLE MEMBERS(
  ME_CMCODE     NCHAR(4),
  ME_OWCODE     NCHAR(3)
)TABLESPACE USERS;
  -- CONSTRAINTS
  ALTER TABLE MEMBERS
  ADD CONSTRAINT ME_CMCODE_OWCODE_PK   PRIMARY KEY(ME_CMCODE, ME_OWCODE)
  ADD CONSTRAINT ME_CMCODE_FK  FOREIGN KEY(ME_CMCODE) REFERENCES CUSTOMERS(CM_CODE)
  ADD CONSTRAINT ME_OWCODE_FK  FOREIGN KEY(ME_OWCODE) REFERENCES OWNERS(OW_CODE);
  -- SYNONYM
  CREATE PUBLIC SYNONYM ME FOR POSDBA.MEMBERS;
  -- GRANT
  GRANT SELECT, INSERT, UPDATE ON POSDBA.MEMBERS TO POSDEV;
  
  -- DATA << DEV
  INSERT INTO ME( ME_CMCODE, ME_OWCODE) VALUES( 'C099', 'W01');
