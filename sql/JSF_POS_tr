/**************************************************************************
 DB PROJECT :: MODELING + DB BUILD
                       DATABASE BASED ON POS
                                             SMART-WEB USING JS FRAMEWORK
**************************************************************************/
/* REQUIREMENTS
    1. USER REQUIREMENTS SPECIFICATION : 시스템 요구사항
    2. FUNCTIONAL REQUIREMENTS SPECIFICATION : 소프트웨어의 기능적 요구사항
       - 상품관리 : 구매와 판매시 수량관리, 가격관리, 기한관리
       - 고객관리 : 포인트 관리
       - 매장관리 :
       - 판매관리
       - 직원관리
       - 매출관리
            :
*/
/* JOB 
  1-1. 상품등록
  1-2. 상품수정
  1-3. 상품삭제
  1-4. 상품조회
  1-5. 상품리스트 조회
  2-1. 고객등록
  2-2. 고객정보수정
  2-3. 고객탈퇴
   :      :
*/
/* TABLE LIST :: MODELING
  0. CATEGORIES TABLE
  1. PRODUCTS TABLE
  2. CUSTOMERS TABLE
  3. OWNERS TABLE
  3-1. STORES TABLE
  4. ORDERS TABLE
  4-1.ORDERSDETAILS TABLE
  4-1-1. POINTS TABLE
*/
/* TABLE DESIGN >> EXCEL */

/* 계정생성 << SYS */
-- DBA
CREATE USER POSDBA IDENTIFIED BY "4321"
DEFAULT TABLESPACE USERS
QUOTA UNLIMITED ON USERS;

GRANT DBA TO POSDBA;

-- DEV
CREATE USER POSDEV IDENTIFIED BY "4321"
DEFAULT TABLESPACE USERS
QUOTA UNLIMITED ON USERS;

GRANT CONNECT, RESOURCE TO POSDEV;
GRANT ALTER ANY TABLE TO POSDEV;

/* TABLE CREATION << DBA */
-- POSDBA.CATEGORIES
CREATE TABLE CATEGORIES(
  CG_CODE   NCHAR(2),
  CG_NAME   NVARCHAR2(20)
)TABLESPACE USERS;
  -- CONSTRAINTS
  ALTER TABLE CATEGORIES
  ADD CONSTRAINT CG_CODE_PK PRIMARY KEY(CG_CODE)
  MODIFY CG_NAME    NOT NULL;
  -- SYNONYM
  CREATE PUBLIC SYNONYM CG FOR POSDBA.CATEGORIES;
  -- GRNAT
  GRANT SELECT, INSERT, UPDATE ON POSDBA.CATEGORIES TO POSDEV;
  
  -- DATA << DEV
  INSERT INTO CG(CG_CODE, CG_NAME) VALUES('P1', '과자');
  INSERT INTO CG(CG_CODE, CG_NAME) VALUES('P2', '음료');
  INSERT INTO CG(CG_CODE, CG_NAME) VALUES('P3', '주류');
  INSERT INTO CG(CG_CODE, CG_NAME) VALUES('P4', '농산');
  INSERT INTO CG(CG_CODE, CG_NAME) VALUES('P5', '수산');
  INSERT INTO CG(CG_CODE, CG_NAME) VALUES('P6', '축산');
  INSERT INTO CG(CG_CODE, CG_NAME) VALUES('P7', '식재료');
  INSERT INTO CG(CG_CODE, CG_NAME) VALUES('P8', '냉동');
  INSERT INTO CG(CG_CODE, CG_NAME) VALUES('O1', '주문진행');
  INSERT INTO CG(CG_CODE, CG_NAME) VALUES('O2', '주문완료');
  INSERT INTO CG(CG_CODE, CG_NAME) VALUES('O9', '주문취소');
  INSERT INTO CG(CG_CODE, CG_NAME) VALUES('S1', '판매가능');
  INSERT INTO CG(CG_CODE, CG_NAME) VALUES('S2', '품절-판매불가');
  INSERT INTO CG(CG_CODE, CG_NAME) VALUES('S3', '단종-판매불가');
  COMMIT;
  
  SELECT * FROM CG;
  
-- POSDBA.PRODUCTS
CREATE TABLE PRODUCTS(
  PR_CODE   NCHAR(5),
  PR_NAME   NVARCHAR2(50)
)TABLESPACE USERS;

  -- CONSTRAINTS
  ALTER TABLE PRODUCTS
  ADD CONSTRAINT PR_CODE_PK PRIMARY KEY(PR_CODE)
  MODIFY PR_NAME    NOT NULL;
  -- SYNONYM
  CREATE PUBLIC SYNONYM PR FOR POSDBA.PRODUCTS;
  -- GRANT 
  GRANT SELECT, INSERT, UPDATE ON POSDBA.PRODUCTS TO POSDEV;
  
