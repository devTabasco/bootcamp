-- POSDBA.ORDERS
DROP TABLE ORDERS;
CREATE TABLE ORDERS(
  OS_OWCODE     NCHAR(3),
  OS_SRCODE     NCHAR(10),
  OS_MECMCODE   NCHAR(4),
  OS_DATE       DATE,
  OS_STATE      NCHAR(2)
)TABLESPACE USERS; 
  -- CONSTRAINTS
  ALTER TABLE POSDBA.ORDERS
  ADD CONSTRAINT OS_OW_SR_MECMCODE_DATE_PK   PRIMARY KEY(OS_OWCODE, OS_SRCODE, OS_MECMCODE, OS_DATE)
  ADD CONSTRAINT OS_OWCODE_SRCODE_FK   FOREIGN KEY(OS_OWCODE, OS_SRCODE) REFERENCES STORES(SR_OWCODE, SR_CODE)
  ADD CONSTRAINT OS_OWCODE_MECODE_FK   FOREIGN KEY(OS_OWCODE, OS_MECMCODE) REFERENCES MEMBERS(ME_OWCODE, ME_CMCODE)
  ADD CONSTRAINT OS_STATE_FK   FOREIGN KEY(OS_STATE) REFERENCES CATEGORIES(CG_CODE)
  MODIFY OS_DATE    DEFAULT SYSDATE NOT NULL
  MODIFY OS_STATE   DEFAULT 'O1' NOT NULL;
  -- SYNONYM
  CREATE PUBLIC SYNONYM OS FOR POSDBA.ORDERS;
  -- GRANT 
  GRANT SELECT, INSERT, UPDATE ON POSDBA.ORDERS TO POSDEV;
  
  -- DATA << DEV
  
  
  SELECT * FROM OS;
  
-- POSDBA.ORDERDETAILS
CREATE TABLE ORDERDETAILS(
  OD_OSOWCODE   NCHAR(3),
  OD_OSSRCODE   NCHAR(10),
  OD_OSMECMCODE NCHAR(4),
  OD_OSDATE     DATE,
  OD_SPPRCODE   NCHAR(5),
  OD_QUANTITY   NUMBER(3,0)
)TABLESPACE USERS;
  -- CONSTRAINTS
  ALTER TABLE ORDERDETAILS
  ADD CONSTRAINT OD_OSOW_SR_MECM_DATE_SPPR_PK    PRIMARY KEY(OD_OSOWCODE, OD_OSSRCODE, OD_OSMECMCODE, OD_OSDATE, OD_SPPRCODE)
  ADD CONSTRAINT OD_OSOW_OSSR_OSMECM_DATE_FK    
        FOREIGN KEY(OD_OSOWCODE, OD_OSSRCODE, OD_OSMECMCODE, OD_OSDATE)
        REFERENCES ORDERS(OS_OWCODE, OS_SRCODE, OS_MECMCODE, OS_DATE)
  ADD CONSTRAINT OD_OSOWCODE_SPPRCODE_FK    
        FOREIGN KEY(OD_OSOWCODE, OD_SPPRCODE)
        REFERENCES STOREPRODUCTS(SP_OWCODE, SP_PRCODE)
  MODIFY OD_QUANTITY    DEFAULT 1 NOT NULL;
  -- SYNONYM
  CREATE PUBLIC SYNONYM OD FOR POSDBA.ORDERDETAILS;
  -- GRANT 
  GRANT SELECT, INSERT, UPDATE ON POSDBA.ORDERDETAILS TO POSDEV;
  
  -- DATA << DEV
  
  
  
  SELECT * FROM OD;
  
/**************************************************************************
 DB PROJECT :: DEVELOPMENT
                       DATABASE BASED ON POS
                                             SMART-WEB USING JS FRAMEWORK
**************************************************************************/

/* JOB CODE :: ACCESS 
    CLIENT   :: OWNERCODE, STORECODE, SECRETCODE
    SERVER   :: (B)STOREINFO 
    DATABASE :: (OW?)OWNERCODE >> (SR?)STORECODE >> (SR?)SECRETCODE >> [INS] HISTORY
                >> [SEL] STOREINFO
    SERVER   :: (S)STOREINFO
*/
  -- 1. OWNERCODE

  -- 2. STORECODE

  -- 3. STORECODE AND SECRETCODE

  -- 4. INS HISTORY

  -- 5. SEL STOREINFO


/* JOB CODE :: GOODS REGISTRATION
    CLIENT   :: (상품코드) (상품명) 구매비용  판매가격  상태코드  분류코드  상품재고
    SERVER   ::(S)STOREINFO (B)GOODSINFO 
    DATABASE :: (PR?)GOODS >> TRUE   >> [INS]SP
                           >> FALSE  >> [INS]PR >> [INS]SP
                >> [SEL] GOODSINFO LIST
    SERVER   :: (L-B)GOODSINFO
    CLIENT   :: 
*/
-- 1. (PR?)GOODS
SELECT COUNT(*)
FROM PR
WHERE PR_CODE = '10001';

-- 1-1. [INS]SP
INSERT INTO SP(SP_PRCODE,SP_OWCODE, SP_COST, SP_PRICE, SP_STATE, SP_GROUP, SP_STOCK)
 VALUES( '10009', 'W01', 2000, 3000, 'S1', 'P1', 100);
COMMIT;
-- 1-2. [INS]SP
INSERT INTO PR(PR_CODE, PR_NAME) VALUES('10009', '오징어땅콩');
INSERT INTO SP(SP_PRCODE, SP_OWCODE, SP_COST, SP_PRICE, SP_STATE, SP_GROUP, SP_STOCK)
 VALUES( '10009', 'W01', 2000, 3000, 'S1', 'P1', 100);
COMMIT;
 -- 테이블 복사
 CREATE TABLE PR_COPY
 AS
 SELECT * FROM PR WHERE 1=2;
 
 SELECT * FROM PR_COPY;
 
 -- 데이터와 함께 복사
 CREATE TABLE PR_COPY2
 AS
 SELECT * FROM PR WHERE PR_NAME LIKE '%깡';
 
 SELECT * FROM PR_COPY2;
 
 -- 데이터 복사
 INSERT INTO PR_COPY(PR_CODE, PR_NAME)
 SELECT * FROM PR;
 
 SELECT * FROM PR_COPY;

