-- POSDBA.CUSTOMERS
CREATE TABLE CUSTOMERS(
  CM_CODE   NCHAR(4),
  CM_NAME   NVARCHAR2(5),
  CM_PHONE  NVARCHAR2(11)
)TABLESPACE USERS;
  -- CONSTRAINTS
  ALTER TABLE CUSTOMERS
  ADD CONSTRAINT CM_CODE_PK PRIMARY KEY(CM_CODE)
  MODIFY CM_NAME    NOT NULL
  MODIFY CM_PHONE   DEFAULT 'NONE' NOT NULL;
  -- SYNONYM
  CREATE PUBLIC SYNONYM CM FOR POSDBA.CUSTOMERS;
  -- GRANT
  GRANT SELECT, INSERT, UPDATE ON POSDBA.CUSTOMERS TO POSDEV;
  
  -- DATA << DEV
  INSERT INTO CM(CM_CODE, CM_NAME, CM_PHONE) VALUES('C001', '김민규', DEFAULT);
