-- POSDBA.STOREPRODUCTS
CREATE TABLE STOREPRODUCTS(
  SP_PRCODE     NCHAR(5),
  SP_OWCODE     NCHAR(3),
  SP_COST       NUMBER(7,0),
  SP_PRICE      NUMBER(7,0),
  SP_STATE      NCHAR(2),
  SP_GROUP      NCHAR(2),
  SP_STOCK      NUMBER(3,0)
)TABLESPACE USERS;
  -- CONSTRAINTS
  ALTER TABLE STOREPRODUCTS
  ADD CONSTRAINT SP_PRCODE_OWCODE_PK    PRIMARY KEY(SP_PRCODE, SP_OWCODE)
  ADD CONSTRAINT SP_PRCODE_FK   FOREIGN KEY(SP_PRCODE) REFERENCES PRODUCTS(PR_CODE)
  ADD CONSTRAINT SP_OWCODE_FK   FOREIGN KEY(SP_OWCODE) REFERENCES OWNERS(OW_CODE)
  ADD CONSTRAINT SP_STATE_FK   FOREIGN KEY(SP_STATE) REFERENCES CATEGORIES(CG_CODE)
  ADD CONSTRAINT SP_GROUP_FK   FOREIGN KEY(SP_GROUP) REFERENCES CATEGORIES(CG_CODE)
  MODIFY SP_COST    DEFAULT 0   NOT NULL
  MODIFY SP_PRICE   DEFAULT 0   NOT NULL
  MODIFY SP_STATE   NOT NULL
  MODIFY SP_GROUP   NOT NULL
  MODIFY SP_STOCK   DEFAULT 0   NOT NULL;
  -- SYNONYM
  CREATE PUBLIC SYNONYM SP FOR POSDBA.STOREPRODUCTS;
  -- GRANT 
  GRANT SELECT, INSERT, UPDATE ON POSDBA.STOREPRODUCTS TO POSDEV;

  -- DATA << DEV
  --P1
  INSERT INTO SP(SP_PRCODE, SP_OWCODE, SP_COST, SP_PRICE, SP_STATE, SP_GROUP, SP_STOCK)
